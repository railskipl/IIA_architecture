<%= form_for [:admin, @libraries], :html => { :multipart => true } do |f| %>
	
	  <p>
	    <%= f.label :book_name %>
	    <%= f.text_field :book_name, :class => 'text' %>
	  </p>
	   <p>
	    <%= f.label :author %>
	    <%= f.text_field :author, :class => 'text' %>
	  </p>
	   <p>
	    <%= f.label :description %>
	    <%= f.text_area :description, :class => 'text' %>
	  </p>
	    
	  <p>
	    <%= f.file_field :book_cover%>
	  </p>
	
	  
	 <%= render :partial =>"button" %>
	
	<% end %>

