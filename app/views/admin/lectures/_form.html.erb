<%= form_for [:admin, @lectures], :html => { :multipart => true } do |f| %>

<p>
	<%= f.label :title, 'Lectures Title'%>
	<%= f.text_field :title%>
</p>

<p>
	<%= f.label :description, 'Lectures description'%>
	<%= f.text_area :description %>
</p>

<p><%= f.file_field :lphoto%></p>

<p><%= f.submit%></p>

<% end %>
